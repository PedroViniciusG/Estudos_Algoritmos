algoritmo "Exercico 01"

var

   ano_atual: inteiro
   nasci_u: inteiro
   idade_u: real

inicio

      escreval ("Em que ano nós estamos?")
      Leia (ano_atual)
      escreval ("Em que ano você nasceu?")
      leia (nasci_u)
      idade_u <- ano_atual - nasci_u
      escreva ("A sua idade é ", idade_u)

fimalgoritmo